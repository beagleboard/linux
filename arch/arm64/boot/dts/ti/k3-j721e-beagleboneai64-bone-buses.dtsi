/* Keys */
/********/
&{/} {
	bone_keys: bone-keys {
		compatible = "gpio-keys";
		autorepeat;

		/* macro: BONE_KEY(<headerpin>) */
		#define BONE_KEY(PX_YY) \
			bone_key_##PX_YY: key_##PX_YY { \
				status = "disabled"; \
				label = #PX_YY ; \
				linux,code = <KEY_UNKNOWN>; \
				gpios = <gpio_##PX_YY GPIO_ACTIVE_HIGH>; \
				pinctrl-names = "default"; \
				pinctrl-0 = <&##PX_YY##_gpio_pd_pin>; \
			}

