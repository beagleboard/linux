#include "am33xx.dtsi"
#include "am335x-pocketbeagle-common.dtsi"

&spi0 {
	#address-cells = <1>;
	#size-cells = <0>;
	status = "okay";

	channel@0 {
		#address-cells = <1>;
		#size-cells = <0>;
		compatible = "spidev";
		reg = <0>;
		spi-max-frequency = <24000000>;
	};

	channel@1 {
		#address-cells = <1>;
		#size-cells = <0>;
		compatible = "spidev";
		reg = <1>;
		spi-max-frequency = <24000000>;
		status = "disabled";
	};
};

&spi1 {
	#address-cells = <1>;
	#size-cells = <0>;
	status = "okay";

	sevenseg: mcp23s18@1 {
		#address-cells = <1>;
		#size-cells = <1>;
		#gpio-cells = <2>;
		compatible = "microchip,mcp23s18";
		gpio-controller;
		microchip,spi-present-mask = <0x01>;
		reg = <1>;
		spi-max-frequency = <1000000>;
	};
};

&i2c2 {
	#address-cells = <1>;
	#size-cells = <0>;
	mma8453@1c {
		status = "okay";
		compatible = "fsl,mma8453";
		reg = <0x1c>;
	};
};

