		status = "disabled";
	};

	gpio_keys: gpio_keys{
		compatible = "gpio-keys";
		pinctrl-names = "default";
		status = "disabled";
		key-volumedown {
			label = "Volume Down Key";
			linux,code = <KEY_1>;
			debounce-interval = <2>;
			gpios = <&ao_gpio_porta 4 GPIO_ACTIVE_LOW>;
		};
		key-volumeup {
			label = "Volume Up Key";
			linux,code = <KEY_2>;
