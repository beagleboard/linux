		aon_reg_dialog: light-dialog-reg {
			compatible = "thead,light-dialog-pmic";
			status = "disabled";
		};

		c910_cpufreq {
			compatible = "thead,light-mpw-cpufreq";
			status = "disabled";
		};

		test: light-aon-test {
			compatible = "thead,light-aon-test";
			status = "disabled";
		};
