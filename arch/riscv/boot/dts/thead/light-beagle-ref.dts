			pinctrl_iso7816: iso7816grp {
				thead,pins = <
					FM_QSPI1_SCLK		0x1	0x208
					FM_QSPI1_D0_MOSI	0x1	0x238
					FM_QSPI1_D1_MISO	0x1	0x238
					FM_QSPI1_D2_WP		0x1	0x238
