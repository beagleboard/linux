		reg = <0>;
		spi-max-frequency = <50000000>;
		w25q,fast-read;
	};

	channel@1 {
		compatible = "rohm,dh2228fv";
		//symlink = "bone/spi/0.1";
		#address-cells = <0x1>;
		#size-cells = <0x0>;
		reg = <0x1>;
		spi-max-frequency = <50000000>;
	};
};

